{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": 0,
    "singleQuote": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-useless-escape": 0
  }
}
